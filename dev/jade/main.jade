include _common/_mixins
doctype html
html(lang="ru")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport" content="width=device-width")
    link(rel="stylesheet" href="css/main.min.css")
    link(rel="apple-touch-icon" sizes="120x120" href="/img/favicons/apple-touch-icon-120x120.png?v=PYY3JrGlvr")
    link(rel="apple-touch-icon" sizes="152x152" href="/img/favicons/apple-touch-icon-152x152.png?v=PYY3JrGlvr")
    link(rel="mask-icon" href="/img/favicons/safari-pinned-tab.svg?v=PYY3JrGlvr")
    link(rel="shortcut icon" href="/img/favicons/favicon.ico?v=PYY3JrGlvr")
    title
      block title
  body
    .wrapper
        include _common/_head
        .content
          .container
            include _common/_breadcrumbs
            h2.content__title Мобильные телефоны
            include _common/_sidebar
            include _common/_sortline
            block content
            include _common/_twocolumns
    include _common/_footer
    script(src="js/main.min.js")

